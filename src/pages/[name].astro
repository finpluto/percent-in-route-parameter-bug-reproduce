---
import type { GetStaticPaths } from "astro";
import Layout from "../layouts/Layout.astro";

export const getStaticPaths = (() => {
    return [
        { params: { name: "Astro%foobar" } }
    ];
}) satisfies GetStaticPaths;

const { name } = Astro.props;

---
<Layout>
    <h1> Hello! {name} </h1>
</Layout>